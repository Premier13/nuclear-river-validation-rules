<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <appSettings>
    <add key="JobStoreType" value="TX"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="TargetEnvironmentName" value="{EnvType}.{EnvNum}"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="SchedulerName" value="ReplicationService.ValidationRules.Scheduler.{EnvType}.{EnvNum}"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="ErmEventsFlowTopic" value="{ErmEventsFlowTopic}"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="EnableJobsRemoteControl" value="true"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>

  <connectionStrings>
    <add name="Erm" connectionString="Data Source={DBHost};Initial Catalog=Erm{EnvNum};Integrated Security=True;Enlist=False"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
    <add name="Facts" connectionString="Data Source={DBHost};Initial Catalog=ValidationRules{EnvNum};Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Aggregates" connectionString="Data Source={DBHost};Initial Catalog=ValidationRules{EnvNum};Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Messages" connectionString="Data Source={DBHost};Initial Catalog=ValidationRules{EnvNum};Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Logging" connectionString="http://logstash.prod.erm.2gis.ru:8194"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Infrastructure" connectionString="Data Source={DBHost};Initial Catalog=ErmInfrastructure;Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="ServiceBus" connectionString="Endpoint=sb://uk-sql01.2gis.local/ERM-{EnvType}{EnvNum},sb://uk-erm-sql02.2gis.local/ERM-{EnvType}{EnvNum};StsEndpoint=https://uk-sql01.2gis.local:9355/ERM-{EnvType}{EnvNum},https://uk-erm-sql02.2gis.local:9355/ERM-{EnvType}{EnvNum};RuntimePort=9354;ManagementPort=9355;OperationTimeout=00:00:05"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>

  <system.serviceModel>
    <client>
      <endpoint address="https://order-validation{EnvNum}.api.test.erm.2gis.ru/Validate.svc/Soap" binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IOrderValidationApplicationService" contract="ErmService.IOrderValidationApplicationService" name="Erm" 
                xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
      <endpoint address="https://validation{EnvNum}.api.test.erm.2gis.ru" binding="webHttpBinding" bindingConfiguration="WebHttpBinding_IRiverRestService" behaviorConfiguration="webEndpoint" contract="ValidationRules.Replication.Comparison.Tests.RiverService.IRiverRestService" name="River" 
                xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </client>
  </system.serviceModel>
</configuration>