<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <appSettings>
    <add key="JobStoreType" value="TX"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="TargetEnvironmentName" value="{EnvType}.{Country}"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="SchedulerName" value="ReplicationService.ValidationRules.Scheduler.{EnvType}.{Country}"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="ErmEventsFlowTopic" value="{ErmEventsFlowTopic}"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="IdentityServiceUrl" value="https://identity.api.prod.erm.2gis.ru"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="LogstashUri" value="tcp://uk-erm-kibana02.2gis.local:8192"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="ReplicationBatchSize" value="10000"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="EnableJobsRemoteControl" value="false"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>

  <connectionStrings>
    <add name="Erm" connectionString="Data Source={DBHost};Initial Catalog=Erm{DBSuffix};Integrated Security=True;Enlist=False"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
    <add name="Facts" connectionString="Data Source={DBHost};Initial Catalog=ValidationRules{DBSuffix};Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Aggregates" connectionString="Data Source={DBHost};Initial Catalog=ValidationRules{DBSuffix};Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Messages" connectionString="Data Source={DBHost};Initial Catalog=ValidationRules{DBSuffix};Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Logging" connectionString="http://logstash.prod.erm.2gis.ru:8195"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Infrastructure" connectionString="Data Source={DBHost};Initial Catalog=ErmInfrastructure;Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="ServiceBus" connectionString="Endpoint=sb://service-bus.prod.erm.2gis.ru/ERM-{EnvType}{Country};StsEndpoint=https://service-bus.prod.erm.2gis.ru:9355/ERM-{EnvType}{Country};RuntimePort=9354;ManagementPort=9355;OperationTimeout=00:00:05"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>

  <system.serviceModel>
    <client>
      <endpoint address="https://order-validation.api.prod.erm.2gis.ru/Validate.svc/Soap" binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IOrderValidationApplicationService" contract="ErmService.IOrderValidationApplicationService" name="Erm"
                xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
      <endpoint address="https://validation.api.prod.erm.2gis.ru" binding="webHttpBinding" bindingConfiguration="WebHttpBinding_IRiverRestService" behaviorConfiguration="webEndpoint" contract="ValidationRules.Replication.Comparison.Tests.RiverService.IRiverRestService" name="River"
                xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </client>
  </system.serviceModel>
  
</configuration>
