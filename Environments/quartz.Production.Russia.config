<?xml version="1.0" encoding="utf-8" ?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
  <schedule>

    <!-- ==================================================================================================================================== -->
    <job>
      <name>ReportingJob</name>
      <group>Replication</group>
      <description>ReportingJob</description>
      <job-type>NuClear.ValidationRules.Replication.Host.Jobs.ReportingJob, 2GIS.NuClear.ValidationRules.Replication.Host</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <simple>
        <name>Запуск ReportingJob через simpletrigger</name>
        <group>Replication</group>

        <job-name>ReportingJob</job-name>
        <job-group>Replication</job-group>

        <repeat-count>-1</repeat-count>
        <repeat-interval>10000</repeat-interval>
      </simple>
    </trigger>
    
    <!-- ==================================================================================================================================== -->
	  <job>
		  <name>ImportFactsFromErmFlow</name>
		  <group>ValidationRules</group>
		  <description>ImportFactsFromErmFlow</description>
		  <job-type>NuClear.ValidationRules.Replication.Host.Jobs.ProcessingJob, 2GIS.NuClear.ValidationRules.Replication.Host</job-type>

		  <durable>true</durable>
		  <recover>false</recover>
	  </job>

	  <trigger>
		  <simple>
			  <name>ImportFactsFromErmFlow</name>
			  <group>ValidationRules</group>

			  <job-name>ImportFactsFromErmFlow</job-name>
			  <job-group>ValidationRules</job-group>

			  <job-data-map>
				  <entry>
					  <key>BatchSize</key>
					  <value>300</value>
				  </entry>
				  <entry>
					  <key>Flow</key>
					  <value>ImportFactsFromErmFlow</value>
				  </entry>
			  </job-data-map>

			  <misfire-instruction>RescheduleNowWithExistingRepeatCount</misfire-instruction>
			  <repeat-count>-1</repeat-count>
			  <repeat-interval>100</repeat-interval>

		  </simple>
	  </trigger>

    <!-- ==================================================================================================================================== -->
    <job>
      <name>CommonEventsFlow</name>
      <group>ValidationRules</group>
      <description>CommonEventsFlow</description>
      <job-type>NuClear.ValidationRules.Replication.Host.Jobs.ProcessingJob, 2GIS.NuClear.ValidationRules.Replication.Host</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <simple>
        <name>CommonEventsFlow</name>
        <group>ValidationRules</group>

        <job-name>CommonEventsFlow</job-name>
        <job-group>ValidationRules</job-group>

        <job-data-map>
          <entry>
            <key>BatchSize</key>
            <value>400</value>
          </entry>
          <entry>
            <key>Flow</key>
            <value>CommonEventsFlow</value>
          </entry>
        </job-data-map>

		    <misfire-instruction>RescheduleNowWithExistingRepeatCount</misfire-instruction>
        <repeat-count>-1</repeat-count>
        <repeat-interval>2000</repeat-interval>

      </simple>
    </trigger>
    
  </schedule>
</job-scheduling-data>
