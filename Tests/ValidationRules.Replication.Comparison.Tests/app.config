<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <connectionStrings>
      <add name="Erm" providerName="System.Data.SqlClient" connectionString="Data Source=uk-sql01;Initial Catalog=Erm21;Integrated Security=True;Enlist=False;" />
      <add name="Messages" providerName="System.Data.SqlClient" connectionString="Data Source=uk-sql01;Initial Catalog=ValidationRules21;Integrated Security=True" />
    </connectionStrings>
    <system.serviceModel>
        <bindings>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IOrderValidationApplicationService" maxReceivedMessageSize="2000000000" receiveTimeout="00:10:00" sendTimeout="00:10:00">
                  <security mode="Transport">
                    <transport clientCredentialType="None" />
                  </security>
                </binding>
            </wsHttpBinding>
            <webHttpBinding>
                <binding name="WebHttpBinding_IRiverRestService" maxReceivedMessageSize="2000000000" receiveTimeout="00:10:00" sendTimeout="00:10:00">
                  <security mode="Transport">
                    <transport clientCredentialType="None" />
                  </security>
                </binding>
            </webHttpBinding>
        </bindings>
        <behaviors>
            <endpointBehaviors>
                <behavior name="webEndpoint">
                    <webHttp/>
                </behavior>
            </endpointBehaviors>
        </behaviors>
        
        <client>
            <endpoint address="https://order-validation21.api.test.erm.2gis.ru/Validate.svc/Soap" binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IOrderValidationApplicationService" contract="ErmService.IOrderValidationApplicationService" name="Erm" />
            <endpoint address="https://validation21.api.test.erm.2gis.ru" binding="webHttpBinding" bindingConfiguration="WebHttpBinding_IRiverRestService" behaviorConfiguration="webEndpoint" contract="ValidationRules.Replication.Comparison.Tests.RiverService.IRiverRestService" name="River" />
        </client>
    </system.serviceModel>
</configuration>
